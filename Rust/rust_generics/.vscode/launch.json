    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo launch lib",
            "cargo": {
                "args": [
                    "build",
                    "--lib"
                ]
            },
            "args": []
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo launch bin",
            "cargo": {
                "args": [
                    "build",
                    "--bin"
                ]
            },
            "args": []
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rust_generics'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rust_hello_objects",
                    "--package=rust_generics"
                ],
                "filter": {
                    "name": "rust_generics",
                    "kind": "bin"
                }
            },
            "args": [
                // "user_arg1",
                // "user_arg2"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rust_hello_objects'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rust_generics",
                    "--package=rust_generics"
                ],
                "filter": {
                    "name": "rust_generics",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}